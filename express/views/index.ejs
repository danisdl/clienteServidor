    <table class= "table table-strip">
      <tr>
        <th>Nombre</th>
        <th>Email</th>
        <th>Tipo</th>
        <th>Acci√≥n</th>
      </tr>
      <%
    if(viewZombies){
      viewZombies.forEach(function(zombie){
        %>
        <tr>
        <td><%= zombie.name %></td>
        <td><%= zombie.email %></td>
        <td><%= zombie.type %></td>
